        .columns.xlarge-4.text-center
          = image_tag 'Temp.png', :alt => 'Temp', :class => "img55"  
        .columns.xlarge-8
          %h3
            %b
              = t('staffing.contingent')
            
          %p
            = t('staffing.contingent-p')
            
