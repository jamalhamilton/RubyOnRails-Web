- content_for :title do
  %link{:href => "https://www.joynus.com/staffing", :rel => "canonical"}
  %title Staffing Service | Joynus Staffing
  %meta{:content => "Experience Joynus. A flexible, fast and convenient way to add workforce per your immediate need.", :name => "Description"}
  %meta{:content => "Staffing Services, temp service, staffing agencies, temp agencies, Joynus", :name => "keywords"} 
  %meta{:content => "/assets/Logo_StackUp-209603de2620f5818742d56fbb0c67de.png", :property => "og:image"}
  %meta{:content => "Website", :property => "og:type"}
  %meta{:content => "summary_large_image", :name => "twitter:card"}
  %meta{:content => "https://www.joynus.com/assets/Logo_StackUp-209603de2620f5818742d56fbb0c67de.png", :name => "twitter:image"}
    
%body
  %section.feature-section.no-spacing
    .feature.animated.fadeIn
      .feature-container
        %img{ data: { interchange:"[#{asset_path('Staffing_mobile.jpg')}, (small)], [#{asset_path('Staffing_final.jpg')}, (medium)], [#{asset_path('Staffing_final.jpg')}, (large)]" } }
        .feature-text
          %h1.text-center.animated.fadeInDown{:style => "color:black"}
            %b 
              = t('footer.lstaffing')
          %h5{:style => "color:black"}
            = t('staffing.staffing-p')
            
  
    .row
      .columns.large-12
        .path
          = active_link_to root_url do
            .srvIco1
              %i.icon-home
          %h5
            = active_link_to t('header.home'), root_url
          .srvIco11
            %i.icon-right  
          %h5
            = active_link_to t('header.services'), services_path
          .srvIco11
            %i.icon-right    
          %h5 
            = t('footer.staffing')
  %section        
    .row
